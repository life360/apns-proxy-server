[loggers]
keys=root

[handlers]
keys=file

[formatters]
keys=simple

[logger_root]
level=INFO
handlers=file

[handler_file]
class=logging.handlers.TimedRotatingFileHandler
args=('/dev/stdout', 'midnight', 1, 0, 'utf-8')
level=INFO
formatter=simple

[formatter_simple]
format=%(asctime)s - %(levelname)s - %(message)s
datefmt=
class=logging.Formatter
